{
  "env": {
    "browser": true,
    "node": true,
    "es6": true
  },
  "extends": ["eslint:recommended", "next", "next/core-web-vitals", "prettier"],
  "ignorePatterns": ["node_modules", ".next", "out", "public"],
  "plugins": ["prettier"],
  "rules": {
    "@next/next/no-img-element": "off",
    "prettier/prettier": "error",
    "no-console": "warn",
    "no-unused-vars": "warn"
  }
}
